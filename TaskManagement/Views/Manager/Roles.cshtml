@model IEnumerable<Entities.Concrete.AppRole>

@{
    ViewData["Title"] = "Roles";
    Layout = "~/Views/Shared/_Manager.cshtml";
}

<h1>Roles</h1>

<p>
    <a asp-action="RoleCreate">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
       
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
     
           
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
          
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
        
        </tr>
}
    </tbody>
</table>